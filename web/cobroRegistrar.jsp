                                <div class="row">
                                    <div class="col-sm-12">
                                        <div class="card">
                                            <div class="card-header">
                                                <h5>Por favor, complete los campos</h5>
                                                <h6 style="color: red">Obs: * Campo requerido</h6>
                                            </div>
                                            <div class="card-body">
                                                <div>
                                                    <%            if (Variables.mensajeRegistradoCheque == 1) {
                                                    %>
                                                    <div class="alert alert-warning alert-dismissible fade show text-center" role="alert" style="float: top;">
                                                        <strong>Genial!</strong> El cheque ha sido registrado.
                                                        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                                                            Ok
                                                        </button>
                                                    </div>
                                                    <script>
                                                        $(".alert").alert('dispose')
                                                    </script>
                                                    <% }%>   
                                                </div>
                                                <hr>
                                                <form action="cobro" method="POST"> 
                                                    <div class="row">                                                      
                                                        <div class="col-md-6">                                                            
                                                            <%----------------------------------------------------------------- --%>                                                            
                                                            <div class="form-group col-md-4">

                                                                <label for="card-holder">Fecha Facturada</label>                                                            
                                                                <%--   <h6 class="text-center text-uppercase">Fecha Facturada</h6>   --%>
                                                                <input id="fechaActual" type="date" name="fechafactxt" class="form-control text-center" aria-label="Card Holder" aria-describedby="basic-addon1" value="" readonly="true">
                                                                &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                          
                                                                <script>
                                                                    window.onload = function () {
                                                                        var fecha = new Date(); //Fecha actual
                                                                        var mes = fecha.getMonth() + 1; //obteniendo mes
                                                                        var dia = fecha.getDate(); //obteniendo dia
                                                                        var ano = fecha.getFullYear(); //obteniendo año
                                                                        if (dia < 10)
                                                                            dia = '0' + dia; //agrega cero si el menor de 10
                                                                        if (mes < 10)
                                                                            mes = '0' + mes //agrega cero si el menor de 10
                                                                        document.getElementById('fechaActual').value = ano + "-" + mes + "-" + dia;
                                                                        document.getElementById('fechaActual2').value = ano + "-" + mes + "-" + dia;
                                                                        document.getElementById('fechaActual3').value = ano + "-" + mes + "-" + dia;
                                                                    }
                                                                </script>

                                                            </div>
                                                            <div class="form-group col-md-4">                                                                
                                                                <input id="card-holder" type="hidden" name="idtxt" class="form-control" aria-label="Card Holder" aria-describedby="basic-addon1" value="<%= cob.getId_cobro()%>">  
                                                                <%--
                                                                <label for="card-holder">N° Cobro</label>
                                                                --%>
                                                                <input id="card-holder" type="hidden" name="idventxt" class="form-control" aria-label="Card Holder" aria-describedby="basic-addon1" value="<%= cob.getId_venta()%>">    
                                                                <%--
                                                                <% if (Variables.nro_cobro1 != 0) {%>
                                                                &nbsp;&nbsp;&nbsp;<input id="card-holder" type="text" name="nrocobtxt" class="form-control text-center" aria-label="Card Holder" aria-describedby="basic-addon1" value="<%= Variables.nro_cobro1%>">
                                                                <% }%>
                                                                --%>
                                                            </div>
                                                            <div class="form-group col-md-4">
                                                                <label for="card-holder">Cta a Cobrar</label>
                                                                <input id="card-holder" type="text" name="idctacobtxt" class="form-control" aria-label="Card Holder" aria-describedby="basic-addon1" value="<%= cob.getId_cta_cobrar()%>">
                                                                <% if (cob.getNro_cta_cobrar() != null) {%><br>
                                                                <label for="card-holder">Nombre o Razon Social:</label>
                                                                &nbsp;&nbsp;&nbsp;<input id="card-holder" type="text" name="ctatxt" class="form-control text-center" aria-label="Card Holder"  readonly="true" aria-describedby="basic-addon1" value="<%= cob.getNombre_razon()%>">
                                                                <%  } else {%>
                                                                &nbsp;&nbsp;&nbsp;<input id="card-holder" type="text" name="ctatxt" class="form-control text-center" aria-label="Card Holder" readonly="true" aria-describedby="basic-addon1" value="">
                                                                <% }%>
                                                            </div>
                                                        </div>
                                                        <div class="col-md-6">                                                           
                                                            <div class="form-group col-md-4">
                                                                <label for="card-holder">N° Factura</label>
                                                                <% if (cob.getNro_factura() != null) {%> 
                                                                &nbsp;&nbsp;&nbsp;<input id="card-holder" type="text" name="nrofacttxt" class="form-control text-center" aria-label="Card Holder" readonly="true" aria-describedby="basic-addon1" value="<%= cob.getNro_factura()%>">
                                                                <%  } else {%>
                                                                &nbsp;&nbsp;&nbsp;<input id="card-holder" type="text" name="nrofacttxt" class="form-control text-center" aria-label="Card Holder" readonly="true" aria-describedby="basic-addon1" value="">
                                                                <% }%> 
                                                            </div>
                                                            <BR>

                                                            <div class="form-group col-md-4">
                                                                <label for="card-holder">Caja</label>
                                                                <% if (Variables.nom_caja_abierta != null) {%>
                                                                &nbsp;&nbsp;&nbsp;<input id="card-holder" type="text" name="cajatxt" class="form-control text-center" aria-label="Card Holder" readonly="true" aria-describedby="basic-addon1" value="<%= Variables.nom_caja_abierta%>">
                                                                <input id="card-holder" type="hidden" name="idcajatxt" class="form-control" aria-label="Card Holder" aria-describedby="basic-addon1" value="<%= cob.getId_caja()%>">          
                                                                <% }%>
                                                            </div>      
                                                            <BR>
                                                            <div class="form-group col-md-4">
                                                                <label for="card-holder">Forma de cobro</label><span><label style="color: red">*</label></span>                                                                                                                             
                                                                <div class="row">
                                                                    <div class="custom-control custom-radio">
                                                                        <% if (cob.getId_fc() == 1 || Variables.fc == 1) {%>
                                                                        <input type="radio" id="cbox1" name="radio" class="only-one" value="1" checked="true">
                                                                        <label for="card-holder cbox1 text-center">Efectivo</label>
                                                                        <%  } else {%>
                                                                        <input type="radio" id="cbox1" name="radio" class="only-one" value="1" checked>
                                                                        <label for="card-holder cbox1 text-center">Efectivo</label>
                                                                        <% }%>
                                                                        &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                                                                        <% if (cob.getId_fc() == 2 || Variables.fc == 2) {%>
                                                                        <input type="radio" id="cbox2" name="radio" class="only-one" value="2" checked="true">
                                                                        <label for="card-holder cbox2 text-center">Transferencia</label>
                                                                        <%  } else {%>
                                                                        <input type="radio" id="cbox2" name="radio" class="only-one" value="2">
                                                                        <label for="card-holder cbox2 text-center">Transferencia</label>
                                                                        <% }%>
                                                                    </div>
                                                                    &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                                                                    <label for="card-holder cbox1 text-center">Cheque</label>
                                                                    &nbsp;&nbsp;&nbsp;&nbsp;                                                                   
                                                                    <button type="submit" class="btn btn-attach" name="accion" value="Cheque">
                                                                        <i class="fas fa-money-check-alt"></i>
                                                                    </button>                                                                   
                                                                </div> 
                                                            </div> 
                                                        </div>
                                                        <div class="col-md-6">
                                                            <div class="row">                                                        
                                                                <div class="col-4 text-center">                                                                    
                                                                    <label for="card-holder" >Nro de Transferencia o Recibo</label>
                                                                    <input type="text" name="conceptxt" class="form-control text-center" aria-label="Card Holder" aria-describedby="basic-addon1" value="">
                                                                </div>
                                                                <div class="col-4 text-center">
                                                                    <label for="card-holder">Monto</label>
                                                                    <input type="text" name="montotxt" class="form-control text-center" aria-label="Card Holder"  aria-describedby="basic-addon1" value="<%= cob.getTotal_cobro()%>">                                                           
                                                                </div> 
                                                            </div> 
                                                            <BR>
                                                            <BR>
                                                            <div class="form-group "> <%-- agregar espacio entre los labels &nbsp --%>                                                                
                                                                <div>
                                                                    <nav class="navbar navbar-dark navbar-expand-md navigation-clean-search">
                                                                        <ul class="nav navbar-nav">
                                                                            <li class="dropdown">
                                                                                <% if (Variables.gua == 0) {%>
                                                                                <input type="submit" name="accion" target="_blank" class="btn btn-light action-button"  value="Guardar">
                                                                                <%}%>
                                                                            </li>
                                                                            <%--
                                                                            <li class="dropdown">
                                                                                <% if (Variables.mod == 0) {%>
                                                                                <input type="submit" name="accion" target="_blank" class="btn btn-light action-button"  value="Modificar">
                                                                                <%}%>
                                                                            </li>   
                                                                            --%>
                                                                            <input type="submit" name="accion" target="_blank" class="btn btn-light action-button"  value="Volver">
                                                                        </ul>
                                                                    </nav>
                                                                </div>
                                                            </div> 
                                                        </div>

                                                    </div>
                                                </form>
                                            </div>
                                        </div>
                                    </div>                                        
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>                
        </div>        

